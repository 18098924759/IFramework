<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <runtime>
    <gcServer enabled="true" />
    <gcConcurrent enabled="true" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Autofac" publicKeyToken="17863af14b0044da" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.0.0" newVersion="4.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <add key="isMemoryMode" value="false" />
    <add key="fileStoreRootPath" value="c:\equeue-store" />
    <add key="chunkSize" value="256" />
    <!--MB-->
    <add key="chunkWriteBuffer" value="256" />
    <!--KB-->
    <add key="enableCache" value="true" />
    <add key="flushInterval" value="100" />
    <!--Milliseconds-->
    <add key="chunkCacheMinPercent" value="40" />
    <add key="messageWriteQueueThreshold" value="10000" />
  </appSettings>
</configuration>